#!/bin/bash -e# Enable jemalloc for reduced memory usage and latency.if [ -z "${LD_PRELOAD+x}" ]; then    LD_PRELOAD=$(find /usr/lib -name libjemalloc.so.2 -print -quit)    export LD_PRELOADfi# If running the rails server then create or migrate existing databaseif [ "${@: -2:1}" == "./bin/rails" ] && [ "${@: -1:1}" == "server" ]; then  ./bin/rails db:preparefiexec "${@}"